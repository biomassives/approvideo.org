{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://www.youtube.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://www.youtube.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/videos",
      "destination": "/api/inputvideo"
    },
    {
      "source": "/api/videos/:id",
      "destination": "/api/update_video"
    },
    {
      "source": "/api/video-categories",
      "destination": "/api/video-categories"
    },
    {
      "source": "/api/video-tags",
      "destination": "/api/video-tags"
    },
    {
      "source": "/api/new-video-item-by-transcript-or-description",
      "destination": "/api/new-video-item-by-transcript-or-description"
    },
    {
      "source": "/videos",
      "destination": "/pages/videos/index"
    },
    {
      "source": "/videos/:id",
      "destination": "/pages/videos/[id]"
    }
  ]
}
